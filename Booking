package com.mycompany.flightsystem;

public class Booking {
    String destination;
    String date;
    String time;

    public Booking(String destination, String date, String time) {
        this.destination = destination;
        this.date = date;
        this.time = time;
    }

    @Override
    public String toString() {
        return "Destination: " + destination + ", Date: " + date + ", Time: " + time;
    }
}
